{
  "name": "admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project admin --web",
  "targets": {
    "docker-dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker compose -f docker-compose.yml up --build admin",
        "cwd": "deployment"
      }
    },
    "docker-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker compose -f docker-compose.prod.yml up --build admin",
        "cwd": "deployment"
      }
    }
  }
}
